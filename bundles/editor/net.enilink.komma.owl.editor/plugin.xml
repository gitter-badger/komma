         name="Datatype Properties">
   </view>
   <view
         category="net.enilink.komma.rdf"
         class="net.enilink.komma.owl.editor.instances.InstanceTreeView"
         icon="icons/full/view16/tree.gif"
         id="net.enilink.komma.owl.editor.views.InstanceTreeView"
         name="Instance Tree"
         restorable="true">
   </view>
   <view
         category="net.enilink.komma.rdf"
         class="net.enilink.komma.owl.editor.instances.InstanceTableView"
         icon="icons/full/view16/table.gif"
         id="net.enilink.komma.owl.editor.views.InstanceTableView"
         name="Instance Table"
         restorable="true">
   </view>
   <category
         id="net.enilink.komma.rdf"
         name="RDF">
   </category>
</extension>
<extension
      point="org.eclipse.ui.perspectives">
   <perspective
         class="net.enilink.komma.owl.editor.OWLPerspective"
         icon="icons/full/obj16/OWLFile.png"
         id="net.enilink.komma.owl.editor.OWLPerspective"
         name="OWL">
   </perspective>
</extension>

</plugin>
