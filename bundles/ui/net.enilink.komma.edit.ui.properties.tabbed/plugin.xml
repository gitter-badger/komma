<plugin>
      <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="net.enilink.komma.edit.ui">
         <propertyCategory
               category="properties">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="net.enilink.komma.edit.ui">
         <propertyTab
               category="properties"
               id="rdfTab"
               label="RDF">
         </propertyTab>
         <propertyTab
               afterTab=""
               category="properties"
               id="propertiesTab"
               label="Default">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="net.enilink.komma.edit.ui">
         <propertySection
               class="net.enilink.komma.edit.ui.properties.tabbed.ExtendedAdvancedPropertiesSection"
               id="propertiesSection"
               tab="propertiesTab">
             <input
                  type="java.lang.Object">
            </input>
         </propertySection>
          <propertySection
                class="net.enilink.komma.edit.ui.properties.tabbed.internal.sections.DetailPropertiesSection"
                id="detailsSection"
                tab="rdfTab">
             <input
                   type="net.enilink.komma.core.IEntity">
             </input>
          </propertySection>
      </propertySections>
   </extension>
</plugin>
